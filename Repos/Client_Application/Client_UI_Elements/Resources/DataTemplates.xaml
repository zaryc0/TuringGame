<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views ="clr-namespace:Client_UI_Elements.View"
                    xmlns:viewModels ="clr-namespace:Client_UI_Elements.ViewModel">
    
    <DataTemplate DataType="{x:Type viewModels:MessageVM}" >

        <views:MessageView/>

    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:SubjectSelection}" >

        <views:SubjectSelectionView/>

    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:TextInputVM}" >

        <views:TextInputView/>

    </DataTemplate>

</ResourceDictionary>